= form_for @navbar_entry do |f|
  - if @navbar_entry.errors.any?
    #error_explanation
      %h2= "#{pluralize(@navbar_entry.errors.count, "error")} prohibited this navbar_entry from being saved:"
      %ul
        - @navbar_entry.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :url
    = f.text_field :url
  .actions
    = f.submit 'Save'
